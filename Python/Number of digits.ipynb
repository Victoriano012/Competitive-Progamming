    "x = int(input())\n",
    "i = 0\n",
    "\n",
    "while True:\n",
    "    i += 1\n",
    "    if x == 10**i:\n",
    "        print(\"The number of digits of \" + str(x) + \" is \" + str(i+1))\n",
    "        break\n",
    "    elif x % 10**i == 0:\n",
    "        print(\"The number of digits of \" + str(x) + \" is \" + str(i))\n",
    "        break"
   ]
  }
